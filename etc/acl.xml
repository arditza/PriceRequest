<?xml version="1.0" ?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Azra_PriceRequest::Request" title="Request" sortOrder="10">
                    <resource id="Azra_PriceRequest::Request_save" title="Save Request" sortOrder="10"/>
                    <resource id="Azra_PriceRequest::Request_delete" title="Delete Request" sortOrder="20"/>
                    <resource id="Azra_PriceRequest::Request_update" title="Update Request" sortOrder="30"/>
                    <resource id="Azra_PriceRequest::Request_view" title="View Request" sortOrder="40"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Azra_PriceRequest::configurations" title="Price Request Configurations"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
